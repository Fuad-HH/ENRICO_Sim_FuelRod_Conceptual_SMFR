FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0.387);

boundaryField
{
    MODINLET
    {
        type           fixedValue;
        value          uniform (0 0 0.5);
    }
    MODOUTLET
    {
        type            pressureInletOutletVelocity;
//        inletValue      $internalField;
        value           $internalField;
    }
    MODTOCLAD
    {
        type            noSlip;
    }
    HEX1c
    {
        type            cyclic;
    }
    HEX1OPc
    {
        type            cyclic;
    }
    HEX2c
    {
        type            cyclic;
    }
    HEX2OPc
    {
        type            cyclic;
    }
    HEX3c
    {
        type            cyclic;
    }
    HEX3OPc
    {
        type            cyclic;
    }

}

